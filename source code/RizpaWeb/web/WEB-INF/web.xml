<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
   <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsersListServlet</servlet-name>
        <servlet-class>servlets.UsersListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReadXMLServlet</servlet-name>
        <servlet-class>servlets.ReadXMLServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EngineServlet</servlet-name>
        <servlet-class>Engine.EngineServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StockTableServlet</servlet-name>
        <servlet-class>servlets.StockTableServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FundsServlet</servlet-name>
        <servlet-class>servlets.FundsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HistoryTableServlet</servlet-name>
        <servlet-class>servlets.HistoryTableServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserStockServlet</servlet-name>
        <servlet-class>servlets.UserStockServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TransactionHistoryServlet</servlet-name>
        <servlet-class>servlets.TransactionHistoryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TransactionServlet</servlet-name>
        <servlet-class>servlets.TransactionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserAlertServlet</servlet-name>
        <servlet-class>servlets.UserAlertServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PendingBuyServlet</servlet-name>
        <servlet-class>servlets.PendingBuyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PendingSellServlet</servlet-name>
        <servlet-class>servlets.PendingSellServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsersListServlet</servlet-name>
        <url-pattern>/userslist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReadXMLServlet</servlet-name>
        <url-pattern>/pages/ReadXMLServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EngineServlet</servlet-name>
        <url-pattern>/pages/EngineServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StockTableServlet</servlet-name>
        <url-pattern>/stocktable</url-pattern>
        <url-pattern>/pages/StockTableServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FundsServlet</servlet-name>
        <url-pattern>/pages/FundsServlet</url-pattern>
        <url-pattern>/totalFunds</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HistoryTableServlet</servlet-name>
        <url-pattern>/historytable</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserStockServlet</servlet-name>
        <url-pattern>/userStockTable</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TransactionHistoryServlet</servlet-name>
        <url-pattern>/transactionHistory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TransactionServlet</servlet-name>
        <url-pattern>/pages/TransactionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserAlertServlet</servlet-name>
        <url-pattern>/updateList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PendingBuyServlet</servlet-name>
        <url-pattern>/buyList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PendingSellServlet</servlet-name>
        <url-pattern>/sellList</url-pattern>
    </servlet-mapping>
</web-app>
